{
  "name": "rabbitmq-client",
  "description": "Node.js RabbitMQ Client.",
  "version": "3.2.0",
  "config": {
    "port": 8000,
    "host": "127.0.0.1",
    "directories": {
      "cache": ".dynamic/cache",
      "logs": ".dynamic/logs"
    },
    "default_user": "guest",
    "default_password": "guest",
    "default_vhost": "/",
    "cluster_nodes": []
  },
  "directories": {
    "doc": "./static/codex",
    "examples": "./examples",
    "bin": "./bin",
    "lib": "./lib",
    "vendor": "./vendor"
  },
  "bin": {
    "rabbitmq-client-start": "./bin/rabbitmq-client-start.js",
    "rabbitmq-client-stop": "./bin/rabbitmq-client-stop.js",
    "rabbitmq-client-restart": "./bin/rabbitmq-client-restart.js",
    "rabbitmq-client-console": "./bin/rabbitmq-client-console.js"
  },
  "scripts": {
    "install": "bash bin/shell/install.sh",
    "publish": "bash bin/shell/install.sh"
  },
  "keywords": [
    "rabbitmq",
    "amqp"
  ],
  "homepage": "http://github.com/UsabilityDynamics/node-rabbitmq-client",
  "author": {
    "name": "Usability Dynamics",
    "email": "info@UsabilityDynamics.com",
    "url": "http://UsabilityDynamics.com"
  },
  "bugs": {
    "url": "http://github.com/UsabilityDynamics/node-rabbitmq-client/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/UsabilityDynamics/node-rabbitmq-client/blob/master/LICENSE-MIT"
    }
  ],
  "main": "lib/rabbitmq-client.js",
  "dependencies": {
    "auto": "auto",
    "winston": "0.7.2",
    "commander": "2.0.0",
    "rabbit": "0.1.0",
    "object-emitter": "*",
    "object-settings": "*",
    "object-validation": "*"
  },
  "devDependencies": {
    "grunt-markdown": "~0.4.0",
    "grunt-contrib-symlink": "*",
    "grunt-mocha-cli": "~1.3.0",
    "grunt-contrib-yuidoc": "*",
    "grunt-contrib-watch": "*",
    "grunt-contrib-less": "*",
    "grunt-contrib-concat": "*",
    "grunt-contrib-clean": "*",
    "grunt-jscoverage": "0.0.3",
    "grunt-shell": "*",
    "express": "*",
    "mocha": "*",
    "should": "*",
    "grunt": "~0.4.1"
  },
  "engines": {
    "node": ">=0.10.21"
  }
}