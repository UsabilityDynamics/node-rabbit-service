{
  "name": "rabbitmq-service",
  "description": "Node.js RabbitMQ Client.",
  "version": "3.2.0",
  "config": {
    "name": "node-rabbit",
    "port": 5672,
    "host": "127.0.0.1",
    "exchange": "",
    "identify": "rabbitmq-service",
    "directories": {
      "config": "static/etc",
      "cache": ".dynamic/cache",
      "pid": ".dynamic/pid",
      "logs": ".dynamic/logs"
    },
    "default_user": "guest",
    "default_password": "guest",
    "default_vhost": "/",
    "cluster_nodes": [],
    "management": {
      "http_log_dir": ".dynamic/logs",
      "port": 15672,
      "host": "127.0.0.1"
    },
    "defaultExchangeName": "",
    "reconnect": true
  },
  "directories": {
    "doc": "./static/codex",
    "examples": "./examples",
    "bin": "./bin",
    "lib": "./lib",
    "vendor": "./vendor"
  },
  "bin": {
    "rabbitmq-service-start": "./bin/rabbitmq-service-start.js",
    "rabbitmq-service-stop": "./bin/rabbitmq-service-stop.js",
    "rabbitmq-service-status": "./bin/rabbitmq-service-status.js",
    "rabbitmq-service-restart": "./bin/rabbitmq-service-restart.js",
    "rabbitmq-service-install": "./bin/rabbitmq-service-install.js",
    "rabbitmq-service-console": "./bin/rabbitmq-service-console.js"
  },
  "scripts": {
    "install": "bash bin/bash/install.sh",
    "pretest": "bash bin/bash/pretest.sh",
    "posttest": "bash bin/bash/posttest.sh",
    "update": "bash bin/bash/update.sh",
    "publish": "bash bin/bash/install.sh"
  },
  "preferGlobal": true,
  "keywords": [
    "rabbitmq",
    "amqp"
  ],
  "homepage": "http://github.com/UsabilityDynamics/node-rabbitmq-service",
  "author": {
    "name": "Usability Dynamics",
    "email": "info@UsabilityDynamics.com",
    "url": "http://UsabilityDynamics.com"
  },
  "bugs": {
    "url": "http://github.com/UsabilityDynamics/node-rabbitmq-service/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/UsabilityDynamics/node-rabbitmq-service/blob/master/LICENSE-MIT"
    }
  ],
  "main": "lib/rabbitmq-service.js",
  "dependencies": {
    "auto": "0.0.2",
    "abstract": "0.0.8",
    "commander": "2.0.0",
    "rabbit": "0.0.5",
    "object-emitter": "0.0.5",
    "object-settings": "0.0.6",
    "object-validation": "0.0.3",
    "winston": "0.7.2"
  },
  "devDependencies": {
    "grunt-markdown": "~0.4.0",
    "grunt-contrib-symlink": "*",
    "grunt-mocha-cli": "~1.3.0",
    "grunt-contrib-yuidoc": "*",
    "grunt-contrib-watch": "*",
    "grunt-contrib-less": "*",
    "grunt-contrib-concat": "*",
    "grunt-contrib-clean": "*",
    "grunt-jscoverage": "0.0.3",
    "grunt-shell": "*",
    "express": "*",
    "mocha": "~1.13.0",
    "should": "*",
    "grunt": "~0.4.1"
  },
  "repository": {
    "url": "git@github.com:UsabilityDynamics/node-rabbitmq-service.git",
    "type": "git"
  },
  "engines": {
    "node": ">=0.10.21"
  }
}
