{
  "name": "rabbit-service",
  "version": "3.2.0",
  "description": "Node.js RabbitMQ Service. We service rabbits.",
  "keywords": [
    "rabbitmq",
    "amqp"
  ],
  "config": {
    "name": "rabbit-service",
    "url": "amqp://guest:guest@localhost:5672/",
    "exchanges": [],
    "cluster_nodes": [],
    "management": {
      "port": 15672,
      "host": "127.0.0.1"
    },
    "defaultExchangeName": "",
    "reconnect": true
  },
  "copyright": "Copyright (c) 2013 Usability Dynamics, Inc.",
  "bin": {
    "rabbit-service": "./bin/rabbit-service.js"
  },
  "scripts": {
    "_install": "bash bin/bash/install.sh",
    "_pretest": "bash bin/bash/pretest.sh",
    "_posttest": "bash bin/bash/posttest.sh",
    "_update": "bash bin/bash/update.sh",
    "_publish": "bash bin/bash/install.sh"
  },
  "directories": {
    "config": "/etc/rabbit-service",
    "cache": "/var/cache/rabbit-service",
    "bin": "./bin",
    "doc": "./static/codex",
    "dynamic": ".dynamic",
    "lib": "./lib",
    "pid": "/var/rabbit-service",
    "log": "/var/log/rabbit-service",
    "vendor": "./vendor"
  },
  "author": {
    "name": "Usability Dynamics",
    "email": "info@UsabilityDynamics.com",
    "url": "http://UsabilityDynamics.com"
  },
  "preferGlobal": true,
  "homepage": "http://github.com/UsabilityDynamics/node-rabbit-service",
  "bugs": {
    "url": "http://github.com/UsabilityDynamics/node-rabbit-service/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/UsabilityDynamics/node-rabbit-service#license"
    }
  ],
  "contributors": [
    {
      "name": "Andy Potanin",
      "email": "andy.potanin@usabilitydynamics.com",
      "url": "http://usabilitydynamics.com"
    },
    {
      "name": "Ivan McCarthy",
      "email": "ivan.mccarthy@usabilitydynamics.com",
      "url": "http://usabilitydynamics.com"
    }
  ],
  "main": "lib/rabbit-service.js",
  "dependencies": {
    "auto": ">=0.0.2",
    "debug": ">=0.7.3",
    "abstract": ">=0.0.8",
    "commander": ">=2.0.0",
    "object-emitter": ">=0.0.5",
    "object-settings": ">=0.0.6",
    "object-validation": ">=0.0.3",
    "winston": ">=0.7.2",
    "string": "~1.6.1",
    "request": "~2.27.0"
  },
  "devDependencies": {
    "grunt-contrib-symlink": "~0.2.0",
    "grunt-contrib-yuidoc": "~0.5.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-less": "~0.8.1",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-jscoverage": "~0.0.3",
    "grunt-markdown": "~0.4.0",
    "grunt-mocha-cli": "~1.3.0",
    "grunt-shell": "~0.5.0"
  },
  "repository": {
    "url": "git@github.com:UsabilityDynamics/node-rabbit-service.git",
    "type": "git"
  },
  "engines": {
    "node": ">=0.10.21"
  }
}
