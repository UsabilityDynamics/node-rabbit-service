{
  "name": "rabbit-service",
  "description": "Node.js RabbitMQ Service. We service rabbits.",
  "version": "3.2.0",
  "config": {
    "name": "node-rabbit",
    "port": 5672,
    "host": "127.0.0.1",
    "exchange": "",
    "identify": "rabbit-service",
    "directories": {
      "config": "static/etc",
      "cache": ".dynamic/cache",
      "pid": ".dynamic/pid",
      "logs": ".dynamic/logs"
    },
    "default_user": "guest",
    "default_password": "guest",
    "default_vhost": "/",
    "cluster_nodes": [],
    "management": {
      "http_log_dir": ".dynamic/logs",
      "port": 15672,
      "host": "127.0.0.1"
    },
    "defaultExchangeName": "",
    "reconnect": true
  },
  "directories": {
    "doc": "./static/codex",
    "examples": "./examples",
    "bin": "./bin",
    "lib": "./lib",
    "vendor": "./vendor"
  },
  "bin": {
    "rabbit-service-start": "./bin/rabbit-service-start.js",
    "rabbit-service-stop": "./bin/rabbit-service-stop.js",
    "rabbit-service-install": "./bin/rabbit-service-install.js",
    "rabbit-service-console": "./bin/rabbit-service-console.js"
  },
  "scripts": {
    "install": "bash bin/bash/install.sh",
    "pretest": "bash bin/bash/pretest.sh",
    "posttest": "bash bin/bash/posttest.sh",
    "update": "bash bin/bash/update.sh",
    "publish": "bash bin/bash/install.sh"
  },
  "preferGlobal": true,
  "keywords": [
    "rabbitmq",
    "amqp"
  ],
  "homepage": "http://github.com/UsabilityDynamics/node-rabbit-service",
  "author": {
    "name": "Usability Dynamics",
    "email": "info@UsabilityDynamics.com",
    "url": "http://UsabilityDynamics.com"
  },
  "bugs": {
    "url": "http://github.com/UsabilityDynamics/node-rabbit-service/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/UsabilityDynamics/node-rabbit-service/blob/master/LICENSE-MIT"
    }
  ],
  "main": "lib/rabbit-service.js",
  "dependencies": {
    "auto": ">=0.0.2",
    "abstract": ">=0.0.8",
    "commander": ">=2.0.0",
    "rabbit-client": "git+ssh://git@github.com:UsabilityDynamics/node-rabbit-client#master",
    "object-emitter": ">=0.0.5",
    "object-settings": ">=0.0.6",
    "object-validation": ">=0.0.3",
    "winston": ">=0.7.2"
  },
  "devDependencies": {
    "grunt-contrib-symlink": "~0.2.0",
    "grunt-contrib-yuidoc": "~0.5.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-less": "~0.8.1",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-jscoverage": "~0.0.3",
    "grunt-markdown": "~0.4.0",
    "grunt-mocha-cli": "~1.3.0",
    "grunt-shell": "~0.5.0"
  },
  "repository": {
    "url": "git@github.com:UsabilityDynamics/node-rabbit-service.git",
    "type": "git"
  },
  "engines": {
    "node": ">=0.10.21"
  }
}
